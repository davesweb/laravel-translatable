{
    "name": "davesweb/laravel-translatable",
    "description": "Add translations to Eloquent models",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.22"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Davesweb\\LaravelTranslatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davesweb\\LaravelTranslatable\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Davesweb\\LaravelTranslatable\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "cs-fixer": [
            "vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ],
        "test": [
            "vendor\\bin\\phpunit.bat tests --colors --testdox"
        ]
    }
}
